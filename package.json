{
  "name": "tinymath",
  "version": "0.0.1",
  "description": "A tiny math expression library with only the stuff I personally care about. Deal with it.",
  "main": "lib/tinymath.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js ./test/**/*.spec.js ",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js ./test/**/*.spec.js "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rashidkpc/tinymath.git"
  },
  "author": "Rashid Khan",
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/rashidkpc/tinymath"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.12.0",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "pegjs": "^0.10.0",
    "webpack": "^3.8.1",
    "yargs": "^10.0.3"
  }
}
